    { "when": { "north": true }, "apply": { "model": "pupdates:block/resin_clump_north" } },
    { "when": { "south": true }, "apply": { "model": "pupdates:block/resin_clump_south" } },
    { "when": { "east": true },  "apply": { "model": "pupdates:block/resin_clump_east" } },
    { "when": { "west": true },  "apply": { "model": "pupdates:block/resin_clump_west" } }
  ]
}
